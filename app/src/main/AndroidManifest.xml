<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.android.allfiltersexample">

    <application
        android:name=".XApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplitChangeExampleActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllFiltersExampleActivity"></activity>
        <activity android:name=".TwoInputFilterExampleActivity"></activity>
        <activity android:name=".GenericFilterExampleActivity"></activity>
        <activity android:name=".VideoToImageExampleActivity"></activity>
    </application>

</manifest>